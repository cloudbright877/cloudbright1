    "@mui/material": "^7.3.7",
    "@radix-ui/react-slider": "^1.3.6",
    "@tanstack/react-virtual": "^3.13.18",
    "@token-icons/react": "^2.13.0",
    "@types/react": "^19.2.7",
    "apexcharts": "^5.3.6",
    "autoprefixer": "^10.4.22",
    "classnames": "^2.5.1",
    "clsx": "^2.1.1",
    "countries-list": "^3.2.2",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.24",
    "lucide-react": "^0.563.0",
    "next": "^16.0.10",
    "next-intl": "^4.6.0",
    "postcss": "^8.5.6",
    "qrcode.react": "^4.2.0",
    "react": "^19.2.3",
    "react-apexcharts": "^1.9.0",
    "react-countdown": "^2.3.6",
    "react-dom": "^19.2.3",
