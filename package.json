{
  "name": "celestian-frontend",
  "version": "1.0.1",
  "description": "Celestian - Crypto Investment Platform UI",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "npx playwright test",
    "test:ui": "npx playwright test --ui",
    "test:headed": "npx playwright test --headed",
    "test:debug": "npx playwright test --debug",
    "test:chromium": "npx playwright test --project=chromium",
    "test:firefox": "npx playwright test --project=firefox",
    "test:webkit": "npx playwright test --project=webkit",
    "test:report": "npx playwright show-report",
    "test:install": "npx playwright install --with-deps",
    "test:unit": "npx tsx tests/unit/convergence/PresetMapper.test.ts && npx tsx tests/unit/convergence/BotValidator.test.ts && npx tsx tests/unit/convergence/ConvergenceController.test.ts && npx tsx tests/unit/DynamicPnLCalculator.test.ts && npx tsx tests/unit/convergence/migration.test.ts && npx tsx tests/unit/TradingBot-convergence.test.ts && npx tsx tests/unit/DailyTargetController.test.ts",
    "test:unit:convergence": "npx tsx tests/unit/convergence/ConvergenceController.test.ts && npx tsx tests/unit/DynamicPnLCalculator.test.ts && npx tsx tests/unit/convergence/migration.test.ts && npx tsx tests/unit/convergence/PresetMapper.test.ts && npx tsx tests/unit/convergence/BotValidator.test.ts",
    "test:integration": "npx playwright test tests/integration/",
    "test:e2e": "npx playwright test tests/specs/e2e/",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:day8": "npx tsx tests/unit/convergence/ConvergenceController.test.ts && npx tsx tests/unit/DynamicPnLCalculator.test.ts",
    "test:day9": "npx tsx tests/unit/convergence/migration.test.ts && npm run test:unit",
    "test:day10": "npm run test:all"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
